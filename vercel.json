{
    "version": 2,
    "builds": [
        {
            "src": "flasksetup.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "25mb",
                "split": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "flasksetup.py"
        }
    ]
}